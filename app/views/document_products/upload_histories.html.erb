<h1>Uploaded Document data</h1>

<%= link_to "CLEAR UPLOAD DATA", removedata_document_products_path, :method => :delete, class: 'btn btn-mini btn-warning' %>
<%= link_to 'UPLOAD FOLDER', bulk_import_document_products_path, class: "btn btn-success btn-small navbar-btn", style:"margin-right: 5px;" %>

<div class="media">
<div class="media-body">
<table class="table table-hover">
   <thead>
    <tr>
      <th>Docnum</th>
      <th>Inspector</th>
      <th>Datetime</th>
    </tr>
  </thead>
  <tbody>
    <% @upload_histories.each do |record| %>
      <tr>
        <td><%= record.docnum %></td>
        <td><%= record.inspector_name %></td>
        <td><%= record.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>

<br>

<%= will_paginate @upload_histories, renderer: BootstrapPagination::Rails  %>


